{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "eslint-config-prettier", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "rules": {
    "strict": ["error", "never"]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
