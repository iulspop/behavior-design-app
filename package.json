{
  "name": "inquire",
  "description": "Inquire is a multi-purpose tool for self-inquiry.",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "remix dev",
    "build": "remix build",
    "start": "remix-serve build",
    "deploy": "fly deploy --remote-only",
    "format": "prettier --write .",
    "type-check": "tsc -b",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "npm run test:env -- vitest --no-threads",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "npm run test:env -- playwright test",
    "test:env": "dotenv -e .env.test --",
    "db:setup": "prisma generate && npm run db:migrate",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:test": "npm run test:env -- prisma db push",
    "db:reset-dev": "run-s db:wipe db:seed dev",
    "db:wipe": "prisma migrate reset --force && prisma migrate dev",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:studio": "prisma studio",
    "prepare": "husky install",
    "gen": "plop"
  },
  "dependencies": {
    "@headlessui/react": "1.7.11",
    "@heroicons/react": "2.0.16",
    "@magic-sdk/admin": "1.8.0",
    "@paralleldrive/cuid2": "2.1.0",
    "@prisma/client": "4.10.1",
    "@remix-run/node": "1.13.0",
    "@remix-run/react": "1.13.0",
    "@remix-run/serve": "1.13.0",
    "domain-functions": "1.5.1",
    "i18next": "22.4.10",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-fs-backend": "2.1.1",
    "i18next-http-backend": "2.1.1",
    "isbot": "3.6.6",
    "magic-sdk": "13.3.1",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-i18next": "12.1.5",
    "remix-forms": "1.5.2",
    "remix-i18next": "5.0.0",
    "tiny-invariant": "1.3.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.6.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "7.6.0",
    "@playwright/test": "1.30.0",
    "@remix-run/dev": "1.13.0",
    "@remix-run/eslint-config": "1.13.0",
    "@remix-run/testing": "1.13.0",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/i18next-fs-backend": "1.1.2",
    "@types/ramda": "0.28.23",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.28.5",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "45.0.2",
    "happy-dom": "8.5.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jsdom": "21.1.0",
    "npm-run-all": "4.1.5",
    "plop": "3.1.2",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "prisma": "4.10.1",
    "tailwindcss": "3.2.7",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.28.5"
  }
}
